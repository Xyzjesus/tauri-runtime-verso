{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Hello World",
  "version": "0.1.0",
  "identifier": "com.tauri.dev",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm web:build"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "Welcome to Tauri!",
        "width": 800,
        "height": 600
      }
    ]
  },
  "bundle": {
    "icon": [
      "../../.icons/icon.ico",
      "../../.icons/icon.png"
    ]
  }
}
