{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Verso Tauri API",
  "version": "0.1.0",
  "identifier": "com.tauri.dev",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm web:build"
  },
  "app": {
    "withGlobalTauri": true
  },
  "bundle": {
    "icon": [
      "../../.icons/icon.ico",
      "../../.icons/icon.png"
    ],
    "externalBin": [
      "../../../../verso/target/debug/versoview"
    ],
    "resources": {
      "../../../../verso/resources": "verso-resources"
    }
  }
}
